{
  "entrypoint": "index.html",
  "shell": "src/app.js",
  "fragments": [
    "src/home.js",
    "src/officers.js",
    "src/committees.js",
    "src/tutoring.js",
    "src/notfound.js",
    "src/components/banner.js",
    "src/components/footer.js"
  ],
  "sources": [
   "404.html",
   "src/app.js",
   "src/tabletop.js",
   "manifest/**",
   "package.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    { 
      "preset": "es6-unbundled",
      "js": {"minify": true},
      "css": {"minify": true},
      "html": {"minify": true}
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
